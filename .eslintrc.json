{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "global-require": 0,
        "no-console": 0,
        "no-unused-vars": 0,
		"no-underscore-dangle": 0,
		"no-use-before-define": 0
    }
}